<script type="text/javascript">
    function verifyRefData()
    {
        var missing = 0;
        if ($('#dont-have-ref:visible').length > 0) {

            if ($('#IssuedBy:visible').val() == '') {
                $('span[data-valmsg-for="IssuedBy"]').show().text('Pole wymagane');
                missing++;
            } else {
                $('span[data-valmsg-for="IssuedBy"]').hide().text('');
            }
            if ($('#IssuingDoctor:visible').val() == '') {
                $('span[data-valmsg-for="IssuingDoctor"]').show().text('Pole wymagane');
                missing++;
            } else {
                $('span[data-valmsg-for="IssuingDoctor"]').hide().text('');
            }
            if ($('span[data-valmsg-for="DateIssued"]').is(":visible") && $('span[data-valmsg-for="DateIssued"]').text() != '') {
                missing++;
            }

        }
        if (missing > 0)
            return false;
        else {
            return true;
        }
    }

