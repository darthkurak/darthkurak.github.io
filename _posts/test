<script>
    function invData(self) {
        console.log(self);
        if (self.value == '1') {
            $('#invCompanyData').slideUp();
            $('#invPersonalData').slideDown();
        } else if (self.value == '2') {
            $('#invPersonalData').slideUp();
            $('#invCompanyData').slideDown();
        }
    }

    $('.AcceptedRules')
        .on('click',
            function() {
                if ($(this).is(':checked')) {
                    $($(this).data('war')).hide();
                } else {
                    $($(this).data('war')).show();
                }
            });

    function submitForm() {
        $('#SearchVisitForm').submit();
    }

    var timeout;
    $('body').on('click',
        '.makeReservationButton',
        function() {
            clearTimeout(timeout);
            timeout = setTimeout(function() {
                    $('#reservationModal  button.close')[0].click();
                },
                600000);
        });
    $('#confirmedReservation').on('click',
        function() {
            // window['submitForm'].apply(window);
        })
    if ($('#Results').is(':parent')) {
        $('.new-visit-form').slideUp('slow');

    }
    $('#SearchAgain').on('click',
        function() {
            $('.new-visit-form select.form-control').each(function() {
                doSelect(this);
            });
            fireDaterange();
            fireDate();
            $('.new-visit-form').slideDown('slow');
            $(this).hide();
            $(document).off("ajaxStop");
            $('p.search-summary').hide();
        });

    $('#SearchPremiumModalBtn').on('click',
        function (e) {
            e.stopPropagation();
            e.preventDefault();
            $('#premiumModal').modal('show');
        });
    $('#SearchPremiumBtn').on('click',
        function (e) {
            e.stopPropagation();
            e.preventDefault();
            $('#premiumModal').modal('hide');
            $('#SearchPremium').val(true);
            $('#acpt-r-p input').prop('checked', true);
            $('#SearchAgain')[0].click();
            setTimeout(function() {
                $('#sbtn')[0].click();
            }, 500);
        });
    $(document).ajaxStop(function() {
        $('p.search-summary').show();
        $('p.search-summary').html($('label[for="City"]').text() +
            ": " +
            $("#City option:selected").text() +
            "&bull;" +
            $('label[for="Department"]').text() +
            ": " +
            $("#checkboxdropdown span.result").text() +
            "&bull;" +
            $('label[for="Specialization"]').text() +
            ": " +
            $("#ListOfSpecialities option:selected").text() +
            "&bull;" +
            $('label[for="Doctor"]').text() +
            ": " +
            $("#checkboxdropdownDoc span.result").text() +
            "&bull;" +
            $('label[for="VisitDateFrom"]').text() +
            ": od " +
            $("#VisitDateFrom").val() +
            " do " +
            $("#VisitDateTo").val() +
            "&bull;" +
            $('label[for="VisitHoursEnum"]').text() +
            ": " +
            $('#VisitHoursEnum option:selected').text());
    });
</script>

