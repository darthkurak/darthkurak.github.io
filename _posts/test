                            }
                        } else {
                            $('#exam-info').hide();
                            $('#Instructions').html('');
                        }

                    }
                });

            var referralData = $.getJSON("/api/EnelmedApi/GetReferralForSearching",
                    { id: this.value, cancelId: $('#VisitToCancelId').val() },
                    function(data) {
                        if (data != null) {
                            hasReferral = data.HasReferral;
                            referralRequired = data.ReferralRequired;
                            referralMandatory = data.ReferralMandatory;
                            referralMandatoryText = data.ReferralMandatoryText;
                            $('#HasReferral').val(hasReferral);
                            $('#ReferralRequired').val(referralRequired);
                            $('#ReferralMandatory').val(referralMandatory);
                            $('#ReferralMandatoryText').val(referralMandatoryText);
                            if (!selectedService ||
                                (selectedService === 0 || window.ListOfServices.value != selectedService)) {
                                $('#BookWithoutReferral').val(false);
                            }
                            if (referralRequired) {
                                $('#Instructions').parent().addClass('examination-prep');
                                $('#Instructions .well').removeClass('well');
                                $('#acpt-r-p').hide();
                                if (hasReferral) {
                                    $('#dont-have-ref-cc').hide();
                                    $('#dont-have-ref-remember').hide();
                                    $('#YouHaveReferral').show();
                                    $('#sbtn').prop('disabled', false);
                                    $('#dont-have-ref').hide();
                                } else {
                                    $('#dont-have-ref-cc').show();
                                    $('#dont-have-ref-remember').show();
                                    $('#YouHaveReferral').hide();
                                    $('#sbtn').prop('disabled', true);
                                    $('#dont-have-ref').show();
                                    $('#sbtn').prop('disabled', !verifyRefData());
                                    if (referralMandatory) {
                                        $('.ref-no').hide();
                                        $('#dont-have-ref-cc').hide();
                                        $('.ref-paper').addClass('col-lg-offset-4').removeClass('col-lg-offset-2');
                                        $('#ref-mandatory-txt').show().find('p').text(referralMandatoryText);
                                    } else {
                                        $('.ref-no').show();
                                        $('.ref-paper').addClass('col-lg-offset-2').removeClass('col-lg-offset-4');
                                        $('#ref-mandatory-txt').hide().find('p').text('');
                                    }
                                }
                            } else {
                                $('#Instructions').parent().removeClass('examination-prep');
                                $('#Instructions>div').addClass('well');
                                $('#acpt-r-p').show();

                                $('#dont-have-ref-cc').hide();
                                $('#dont-have-ref-remember').hide();
                                $('#YouHaveReferral').hide();
                                $('#sbtn').prop('disabled', false);
                                $('#dont-have-ref').hide();
                            }
                        } else {
                            $('#dont-have-ref-cc').hide();
                            $('#dont-have-ref-remember').hide();
                            $('#YouHaveReferral').hide();
                            $('#sbtn').prop('disabled', false);
                            $('#dont-have-ref').hide();
                            $('#Instructions').parent().removeClass('examination-prep');
                            $('#Instructions>div').addClass('well');
                            $('#acpt-r-p').show();
                        }
                    });
            }

        });

        $('#ForeignLanguageDoctor').change(function(event) {

            var dropdown = $('#checkboxdropdownDoc .choose-group');
            dropdown.html('');
            if (window.choosenDepartments &&
                window.choosenDepartments.length > 0 &&
                $('#ListOfTypes').val() &&
                $('#ListOfServices').val()) {
                $.ajax({
                    url: '/api/EnelmedApi/GetDoctorsByDepartmentIdAndByServiceId',
                    type: 'POST',
                    data: {
                        "ServiceId": $('#ListOfServices').val(),
                        "ForeignLanguage": seng ? true : $('#ForeignLanguageDoctor').is(':checked'),
                        "DepartmentsId": (window.choosenDepartments)
                    },
                    success: function(obj) {
                        if (window.repeatSearch) {
                            setTimeout(function() {
                                    $('#Loader').show();
                                    $('#Overlay').show();
                                },
                                1);
                        }
                        if (obj != null) {
                            unblockControl($('#checkboxdropdownDoc button'));
                            $.each(obj,
                                function(i, value) {

                                    dropdown.append('<label><input type="checkbox" name="Doctor" value="' +
                                        value.DoctorId +
                                        '">' +
                                        '<span value="' +
                                        value.DoctorId +
                                        '">' +
                                        value.DoctorData +
                                        '</span></label>');
                                });
                            if (window.reSelectDoc) {
                                dropdown.find('input').prop('checked', true);
                                triggerEvent('#confirmDoctor', 'click');
                                triggerEvent($('#confirmDoctor').closest('.dropdown-checkboxes')
                                    .find('.js-accept-dropdown'),
                                    'click');
                            } else if (serialized && selectedCity.Doctor) {
                                dropdown.find('input').each(function() {
                                    if ($.inArray(Number($(this).attr('value')), selectedCity.Doctor) != -1) {
                                        $(this).prop('checked', true);
                                    }
                                });
                                triggerEvent('#confirmDoctor', 'click');
                                triggerEvent($('#confirmDoctor').closest('.dropdown-checkboxes')
                                    .find('.js-accept-dropdown'),
                                    'click');
                            } else if (doctorId) {
                                dropdown.find('input').each(function() {
                                    if ($(this).attr('value') == doctorId) {
                                        $(this).prop('checked', true);
                                        triggerEvent('#confirmDoctor', 'click');
                                        triggerEvent($('#confirmDoctor').closest('.dropdown-checkboxes')
                                            .find('.js-accept-dropdown'),
                                            'click');
                                    }
                                });

                            } else if (obj.length > 0) {
                                dropdown.find('input').prop('checked', true);
                                triggerEvent('#confirmDoctor', 'click');
                                triggerEvent($('#confirmDoctor').closest('.dropdown-checkboxes')
                                    .find('.js-accept-dropdown'),
                                    'click');
                            }
                            unblockControl('#checkboxdropdownDoc button');
                            unblockControl('#ForeignLanguageDoctor');
                            window.doctorsLoaded = true;
                            if (window.repeatSearch) {
                                window.repeatSearch = false;
                                $('#SearchVisitForm').submit();
                            }
                        }
                    }

                });}

        });

        if (!serialized) {
            $('input[id="' +
                'PreferredVisitHours' +
                '"][value="' +
                0 +
                '"]').prop('checked', true);
        }


        $('#PageSize > li > a').on('click',
            function(e) {
                e.preventDefault();
                $(document).off("ajaxStop");
            });


    });

    function getQueryVariable(variable) {
        var query = window.location.search.substring(1);
        var vars = query.split("&");
        for (var i = 0; i < vars.length; i++) {
            var pair = vars[i].split("=");
            if (pair[0] == variable) {
                return pair[1];
            }
        }
        return (false);
    }

    function DoSearch() {
        setTimeout(function() {
                $('#Loader').show();
                $('#Overlay').show();
            },
            1);
        if (!window.doctorsLoaded) {
            window.repeatSearch = true;
            setTimeout(function() {
                    $('#Loader').show();
                    $('#Overlay').show();
                },
                100);
            return false;
        }
        window.doctorsLoaded = false;
    }

    function VerifyEnelFast() {
        var all = $('#checkboxdropdownDoc input[name="Doctor"]');
        $('#CanSearchPremium').val(all.length !== all.filter(':checked').length);
        return true;
    }

    $(document).ready(function() {
        $('#limitOnlineModal').modal();
        $('#limitModal').modal();
        fireDaterange();
        fireDate();
    });

    function validateRules(e) {
        var $r = $('#AcptRul:visible');
        if ($r.length > 0) {
            if ($r.is(':checked')) {
                $r.parent().removeClass('text-warning');
            } else {
                $r.parent().css('color', '#d50000!important');
                e.preventDefault();
            }
        }
    }

    var reservationData;

    function makeReservationSelect(doctorId,
        visitDate,
        visitDateStart,
        visitDateEnd,
        roomId,
        serviceId,
        popupDescription,
        langId,
        sessionServerGuid,
        hasPay,
        payBtnText,
        paperReferral,
        referralId,
        bookWithoutReferral,
        premium) {
        reservationData = {'doctorId': doctorId,
            'visitDate': visitDate,
            'visitDateStart': visitDateStart,
            'visitDateEnd': visitDateEnd,
            'roomId': roomId,
            'popupDescription': popupDescription,
            'langId': langId,
            'sessionServerGuid': sessionServerGuid,
            'hasPay': hasPay,
            'payBtnText': payBtnText,
            'serviceId': serviceId,
            'paperReferral' : paperReferral,
            'referralId' : referralId,
            'bookWithoutReferral': bookWithoutReferral,
            'premium': premium
        }
    }
    function continueChange(event, visitId) {
        event.preventDefault();
        event.stopPropagation();
        $('#changeVisitModal').modal('hide');
        $('#changeVisitModal').on('hidden.bs.modal',
            function () {
                $('#reservationModal').modal('show');
                makeReservation(reservationData.doctorId,
                    reservationData.visitDate,
                    reservationData.visitDateStart,
                    reservationData.visitDateEnd,
                    reservationData.roomId,
                    reservationData.serviceId,
                    visitId,
                    reservationData.popupDescription.slice(1, -1),
                    reservationData.langId,
                    reservationData.sessionServerGuid,
                    reservationData.hasPay,
                    reservationData.payBtnText,
                    reservationData.paperReferral,
                    reservationData.referralId,
                    reservationData.bookWithoutReferral,
                    reservationData.premium);
                $('#changeVisitModal').off('hidden.bs.modal');
            });
        return true;
    }
</script>
